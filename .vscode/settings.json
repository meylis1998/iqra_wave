{
    "dart.lineLength": 80,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.dart_tool": true,
        "**/.git": true,
        "**/.gradle": true,
        "**/.idea": true,
        "**/.pub": true,
        "**/ios/.symlinks": true,
        "**/test/.test_coverage": true
    },
    "search.exclude": {
        "**/.dart_tool": true,
        "**/.git": true,
        "**/.gradle": true,
        "**/.pub": true,
        "**/ios/.symlinks": true,
        "**/test/.test_coverage": true
    },
    "terminal.integrated.cwd": "${workspaceFolder}"
}
